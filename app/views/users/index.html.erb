<div class="row">
	<div class="col-lg-3">
<%= render 'users/list', user: @user %>
<%= render 'books/list', bookn: @bookn %>
</div>
<div class="col-lg-9">
<h1>Users</h1>
<table class="table">
	<thead>
		<th>image</th>
		<th>name</th>
		<th></th>
	</thead>
	<% @userlist.each do |user| %>
	<tbody>
		<th><%= attachment_image_tag user, :profile_image, :fill,35,35 %></th>
		<td><%= user.name %></td>
		<td><%= link_to "show", user_path(user.id) %></td>
	</tbody>

	<% end %>
</table>
</div>
</div>